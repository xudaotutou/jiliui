{
  "name": "jiliui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./style": {
      "require": "./packages/style.cjs"
    }
  },
  "scripts": {
    "b_css_pre": "postcss --config ./packages --base packages --dir dist",
    "b_base": "pnpm b_css_pre ./packages/components/base/base.css  && jss  convert ./dist/components/base/base.css -f js -e cjs > ./dist/components/base/base.cjs",
    "b_backToTop": "pnpm b_css_pre ./packages/components/backToTop/backToTop.css  && jss  convert ./dist/components/backToTop/backToTop.css -f js -e cjs > ./dist/components/backToTop/backToTop.cjs",
    "b_Jlayout": "pnpm b_css_pre ./packages/components/layout/Jlayout.css  && jss  convert ./dist/components/layout/Jlayout.css -f js -e cjs > ./dist/components/layout/Jlayout.cjs",
    "dev": "pnpm b_Jlayout && pnpm b_base && pnpm b_backToTop && vite",
    "build": "vue-tsc && vite build && pnpm b_base && pnpm b_Jlayout && pnpm b_backToTop",
    "test": "vitest run",
    "test:w": "vitest -w",
    "coverage": "vitest run --coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "jiliui": "link:",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/runtime-core": "^3.2.47",
    "@vue/test-utils": "2.0.0-rc.18",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-tailwindcss": "^3.8.3",
    "eslint-plugin-vue": "^9.9.0",
    "happy-dom": "^8.2.2",
    "husky": "^8.0.3",
    "jss": "^10.9.2",
    "jss-cli": "^6.0.2",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "prejss-cli": "^0.3.3",
    "tailwindcss": "^3.2.4",
    "typescript": "*",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.2",
    "vitepress": "1.0.0-alpha.43",
    "vitest": "^0.28.3",
    "vue-eslint-parser": "^9.1.0",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.11",
    "yarnhook": "^0.6.0"
  },
  "husky": {
    "hooks": {
      "post-checkout": "yarnhook",
      "post-merge": "yarnhook",
      "post-rewrite": "yarnhook"
    }
  }
}
